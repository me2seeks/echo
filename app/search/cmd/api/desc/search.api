syntax = "v1"

import (
	"feeds/feeds.api"
	"users/users.api"
)

type (
	SearchReq {
		Keyword  string `form:"keyword"`
		Page     int64  `form:"page"`
		PageSize int64  `form:"pageSize"`
	}
)

@server (
	prefix: search
	group:  user
)
service search {
	@doc "search users"
	@handler search
	post /users (SearchReq) returns (SearchUsersResp)
}

@server (
	prefix: search
	group:  feed
)
service search {
	@doc "search feeds"
	@handler search
	post /feeds (SearchReq) returns (SearchFeedsResp)
}

